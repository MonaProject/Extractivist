    "http://dbpedia.org/property/source" : [ { "type" : "literal", "value" : "\u2014 Poem written by Joseph Stilwell in 1944" , "lang" : "en" } ] ,
    "http://dbpedia.org/property/quote" : [ { "type" : "literal", "value" : "I know I've still to suffer," , "lang" : "en" } ,
      { "type" : "literal", "value" : "And kicked him in the pants." , "lang" : "en" } ,
      { "type" : "literal", "value" : "At last I\u2019ve had my chance." , "lang" : "en" } ,
      { "type" : "literal", "value" : "I have waited long for vengeance," , "lang" : "en" } ,
      { "type" : "literal", "value" : "As he struggled not to screech." , "lang" : "en" } ,
      { "type" : "literal", "value" : "The old harpoon was ready" , "lang" : "en" } ,
      { "type" : "literal", "value" : "At last I've had my innings" , "lang" : "en" } ,
      { "type" : "literal", "value" : "With aim and timing true," , "lang" : "en" } ,
      { "type" : "literal", "value" : "I've wrecked the Peanut's face." , "lang" : "en" } ,
      { "type" : "literal", "value" : "The little bastard shivered," , "lang" : "en" } ,
      { "type" : "literal", "value" : "But oh! the blessed pleasure!" , "lang" : "en" } ,
      { "type" : "literal", "value" : "And lost the power of speech." , "lang" : "en" } ,
      { "type" : "literal", "value" : "And stung him through and through." , "lang" : "en" } ,
      { "type" : "literal", "value" : "I sank it to the handle," , "lang" : "en" } ,
      { "type" : "literal", "value" : "And run a weary race," , "lang" : "en" } ,
      { "type" : "literal", "value" : "I\u2019ve looked the Peanut in the eye" , "lang" : "en" } ,
      { "type" : "literal", "value" : "For all weary battles," , "lang" : "en" } ,
      { "type" : "literal", "value" : "And laid the Peanut low." , "lang" : "en" } ,
      { "type" : "literal", "value" : "His face turned green and quivered" , "lang" : "en" } ,
      { "type" : "literal", "value" : "For all my hours of woe," , "lang" : "en" } ] ,
